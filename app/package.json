  },
  "dependencies": {
    "material-ui": "^0.17.1",
    "moment": "^2.18.1",
    "normalizr": "^3.2.2",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
